{
  "name": "your-name/inpost-shipment-creator",
  "description": "A robust PHP script to create InPost courier shipments and order a courier.",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\Api\\": "App/Api/",
      "App\\Logger\\": "App/Logger/",
      "App\\Service\\": "App/Service/",
      "App\\Contracts\\": "App/Contracts/",
      "App\\Enums\\": "App/Enums/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
